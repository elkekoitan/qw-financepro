      "NEXT_PUBLIC_SUPABASE_ANON_KEY": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6Im1ieGtkZ2tycHVrZGt3cW5xamhtIiwicm9sZSI6ImFub24iLCJpYXQiOjE3MTUyMzcyNDUsImV4cCI6MjAzMDgxMzI0NX0.Hcr2M4OnOJYwcwQ2HY-r_JghpKKoQX2WmCyYk51GwQc",
