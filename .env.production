# Environment variables for FinancialPro Project - Production Environment

# Server port (default is 3000)
PORT=3000

# Supabase configuration
NEXT_PUBLIC_SUPABASE_URL=https://prod.gjnelwfpcllmmyhsxhmt.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_production_anon_key
SUPABASE_SERVICE_ROLE=your_production_service_role_key
SUPABASE_DB_PASSWORD=your_production_db_password
SUPABASE_HOST=prod.gjnelwfpcllmmyhsxhmt.supabase.co
SUPABASE_USER=postgres.production

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_production_google_client_id
GOOGLE_CLIENT_SECRET=your_production_google_client_secret

# Callback URLs
SUPABASE_CALLBACK_URL=https://financepro.com/auth/v1/callback
SUPABASE_STORAGE_URL=https://prod.gjnelwfpcllmmyhsxhmt.supabase.co/storage/v1/s3
SUPABASE_STORAGE_REGION=eu-central-1

# JWT secret for authentication
JWT_SECRET=your_production_jwt_secret

# Redis configuration
REDIS_URL=redis://prod-redis:6379
REDIS_PASSWORD=your_production_redis_password

# SendGrid configuration
SENDGRID_API_KEY=your_production_sendgrid_api_key
SENDGRID_FROM_EMAIL=noreply@financepro.com

# Application URLs
NEXT_PUBLIC_APP_URL=https://financepro.com

# Node environment
NODE_ENV=production

# Sentry configuration for error tracking
SENTRY_DSN=your_production_sentry_dsn
SENTRY_ENVIRONMENT=production

# New Relic configuration for performance monitoring
NEW_RELIC_LICENSE_KEY=your_production_newrelic_key
NEW_RELIC_APP_NAME=FinancePro-Production

# Google Analytics configuration
GA_MEASUREMENT_ID=your_production_ga_id 