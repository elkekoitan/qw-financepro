# Environment variables for FinancialPro Project

# Server port (default is 3001)
PORT=3001

# Supabase configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE=your_supabase_service_role_key
SUPABASE_DB_PASSWORD=your_supabase_db_password
SUPABASE_HOST=your_supabase_host
SUPABASE_USER=your_supabase_user

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Callback URLs
SUPABASE_CALLBACK_URL=your_supabase_callback_url
SUPABASE_STORAGE_URL=your_supabase_storage_url
SUPABASE_STORAGE_REGION=eu-central-1

# JWT secret for authentication
JWT_SECRET=your_jwt_secret

# Redis configuration
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=your_redis_password

# SendGrid configuration
SENDGRID_API_KEY=your_sendgrid_api_key
SENDGRID_FROM_EMAIL=noreply@financepro.com

# Application URLs
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Node environment
NODE_ENV=development

# Sentry
NEXT_PUBLIC_SENTRY_DSN=your_sentry_dsn
NEXT_PUBLIC_SENTRY_ENVIRONMENT=development

# New Relic
NEW_RELIC_LICENSE_KEY=your_new_relic_license_key
NEW_RELIC_APP_NAME=FinancePro
NEW_RELIC_REGION=EU

# Google Analytics
GA_MEASUREMENT_ID=your_ga_measurement_id 